<?xml version="1.0"?>

<launch>
  <!-- Load joint controller configurations from YAML file to parameter server -->
  <rosparam file="$(find haircut_moveit_config)/config/trajectory_controllers.yaml" command="load"/>

  <arg name="controller_name" default="follow_trajectory_controller"/>
  <!-- Load the controllers -->
  <node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false"
    output="screen" args="joint_state_controller $(arg controller_name)"/>

</launch>